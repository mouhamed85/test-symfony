{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} | Créer un article{% endblock %}

{% block body %}
<div class="container">
    <h1>Créer un nouvel article</h1>

    {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit" class="btn">{{ button_label|default('Créer l\'article') }}</button>
    {{ form_end(form) }}
</div>
{% endblock %}
